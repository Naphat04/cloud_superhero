generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model work_cloud {
  id              Int    @id @default(autoincrement())
  img_art         String @db.Text
  name_of_headimg String @db.VarChar(50)
  texter_img      String @db.Text
}
